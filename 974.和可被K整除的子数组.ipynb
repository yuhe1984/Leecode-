    "class Solution:\n",
    "    def subarraysDivByK(self, A, K):\n",
    "        s = [0] * (len(A) + 1)\n",
    "        knct = [0] * K\n",
    "        for i in range(len(A)):\n",
    "            s[i + 1] = s[i] + A[i]\n",
    "        for item in s:\n",
    "            knct[item % K] += 1\n",
    "        return sum(x * (x - 1)  // 2 for x in knct)"
   ]

   "source": [
    "class Solution:\n",
    "    def reverseWords(self, s: str) -> str:\n",
    "        s1 = s.split(' ')\n",
    "        s2 = ''\n",
    "        for i in s1:\n",
    "            s2 += i[::-1]\n",
    "            s2 += ' '\n",
    "        return s2.strip()"
   ]

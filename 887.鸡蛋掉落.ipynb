   "source": [
    "class Solution:\n",
    "    def superEggDrop(self, K: int, N: int) -> int:\n",
    "        dp = [0] * (K + 1)\n",
    "        m = 0\n",
    "        while dp[K] < N:\n",
    "            m += 1\n",
    "            for k in range(K, 0, -1):\n",
    "                dp[k] = dp[k - 1] + dp[k] + 1\n",
    "        return m"
   ]

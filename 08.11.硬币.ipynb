   "source": [
    "class Solution:\n",
    "    def waysToChange(self, n):\n",
    "        mod = 10**9 + 7\n",
    "        coins = [25, 10, 5, 1]\n",
    "\n",
    "        f = [1] + [0] * n\n",
    "        for coin in coins:\n",
    "            for i in range(coin, n + 1):\n",
    "                f[i] += f[i - coin]\n",
    "        return f[n] % mod"
   ]
